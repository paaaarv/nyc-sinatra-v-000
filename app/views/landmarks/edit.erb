<h2> Update <%=@landmark.name%></h2>

<form action = "/landmarks/<%= @landmark.id%>" method = "POST">
  <input type = "hidden" name = "_method" value = "PATCH">
  <label> Landmark Name: </label><br>
  <input type = 'text' name = "landmark[name]" id = "name"> <br>
  <label> Year Completed: </label> <br>
  <input type = "text" name = "landmark[year_completed]" id = "year_completed" value = "<%=@landmark.year_completed.to_s%>"> <br>
  <input type = "submit" value = "Edit Landmark">
</form>
